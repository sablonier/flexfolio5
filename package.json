{
  "name": "flexfolio5",
  "version": "0.0.1",
  "description": "Portfolio/Bootstrap theme for Bolt CMS",
  "source": [
    "src/css/flexfolio.scss",
    "src/css/fonts/rubik.scss",
    "src/css/fonts/publicsans.scss",
    "src/js/flexfolio.js"
  ],
  "scripts": {
    "watch": "parcel watch --no-content-hash --no-source-maps --dist-dir dist",
    "build": "parcel build --no-content-hash --no-source-maps --dist-dir dist --no-cache && cp -R ./src/favicon ./dist/favicon && cp ./src/css/custom.css ./dist/css/custom.css"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@parcel/transformer-sass": "^2.1.1",
    "parcel": "^2.5.0",
    "sass": "^1.46.0"
  },
  "dependencies": {
    "@fontsource/brygada-1918": "^4.5.8",
    "@fontsource/public-sans": "^4.5.0",
    "@fontsource/rubik": "^4.5.0",
    "@popperjs/core": "^2.11.2",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.7.2",
    "cookie-notice": "^1.3.9"
  }
}
